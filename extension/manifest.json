{
  "manifest_version": 2,

  "name": "Security Chrome Extension",
  "description": "This extension will not allow you to access sites from the blocklist",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
   "activeTab",
   "storage",
   "webRequest",
   "webRequestBlocking",
   "<all_urls>"
  ]
}